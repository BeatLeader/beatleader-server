    <Content Remove="bs-simulator\ReplayRecalculatorRunner\**" />
    <Content Remove="bs-simulator\ReplayRecalculator\obj\**" />
    <Content Remove="ReplayDecoder\**" />
    <EmbeddedResource Remove="beatleader-parser\**" />
