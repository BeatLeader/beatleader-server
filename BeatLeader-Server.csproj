    <Content Remove="BeatLeader-Server-Sockets\**" />
    <EmbeddedResource Remove="BeatLeader-Server-Sockets\**" />
    <None Remove="BeatLeader-Server-Sockets\**" />
    <None Remove="ReplayDecoder\**" />
